int[] previousSmaller(int[] arr) {
    int n = arr.length;
    int[] pse = new int[n];
    Stack<Integer> st = new Stack<>();

    for (int i = 0; i < n; i++) {
        while (!st.isEmpty() && st.peek() >= arr[i]) {
            st.pop();
        }
        pse[i] = st.isEmpty() ? -1 : st.peek();
        st.push(arr[i]);
    }
    return pse;
}
